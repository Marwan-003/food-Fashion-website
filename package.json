{
  "devDependencies": {
    "gulp": "^5.0.0",
    "gulp-autoprefixer": "^9.0.0",
    "gulp-notify": "^4.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.78.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "build-tail": "npx tailwindcss -i ./src/styles/css/tailwind.css -o ./dist/styles/tailwind.css --watch",
    "build-tsc": "tsc -w",
    "build-gulp": "gulp --gulpfile src/gulpfile.js watch",
    "build": "npm-run-all --parallel  build-tail build-tsc build-gulp "
  },
  "type": "module"
}
